{
  "manifest_version": 3,
  "name": "AudioBookBay Magnet Assistant",
  "description": "Creates magnet links for AudioBookBay pages without logging in.",
  "version": "2.0.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.audiobookbay.nl/abs/*",
        "*://*.audiobookbay.is/abs/*",
        "*://*.audiobookbay.se/abs/*"
      ],
      "js": [
        "abbma.js"
      ]
    }
  ],
  "content_security_policy": {},
  "browser_specific_settings": {
    "gecko": {
      "id": "{16ec1e5b-2ec8-4186-89fb-ca8f548d6178}",
      "strict_min_version": "109.0"
    }
  }
}