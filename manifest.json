{
	"manifest_version": 2,
	"name": "AudioBookBay Magnet Assistant",
	"description": "Creates magnet links for AudioBookBay pages without logging in.",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": [
				"*://*.audiobookbay.nl/audio-books/*"
				"*://*.audiobookbay.ws/audio-books/*"
				"*://*.audiobookbay.fi/audio-books/*"
			],
			"js": [
				"abbma.js"
			]
		}
	]
}
